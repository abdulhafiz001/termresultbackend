APP_NAME=Laravel
APP_ENV=local
APP_KEY=
APP_DEBUG=true
APP_URL=http://localhost

APP_LOCALE=en
APP_FALLBACK_LOCALE=en
APP_FAKER_LOCALE=en_US

APP_MAINTENANCE_DRIVER=file
# APP_MAINTENANCE_STORE=database

# PHP_CLI_SERVER_WORKERS=4

BCRYPT_ROUNDS=12

LOG_CHANNEL=stack
LOG_STACK=single
LOG_DEPRECATIONS_CHANNEL=null
LOG_LEVEL=debug

# Local dev can use sqlite, but production should use MySQL.
DB_CONNECTION=sqlite
# For production (recommended):
# APP_ENV=production
# APP_DEBUG=false
# LOG_LEVEL=info
# APP_URL=https://api.termresult.com
# DB_CONNECTION=mysql
# DB_HOST=127.0.0.1
# DB_PORT=3306
# DB_DATABASE=termresult_central
# DB_USERNAME=your_db_user
# DB_PASSWORD=your_db_password

SESSION_DRIVER=database
SESSION_LIFETIME=120
SESSION_ENCRYPT=false
SESSION_PATH=/
SESSION_DOMAIN=null

BROADCAST_CONNECTION=log
FILESYSTEM_DISK=local
# On cPanel/shared hosting (recommended): use sync queue so you don't need a worker.
# QUEUE_CONNECTION=sync
QUEUE_CONNECTION=database
DB_QUEUE_CONNECTION=mysql

CACHE_STORE=database
# CACHE_PREFIX=
DB_CACHE_CONNECTION=mysql
DB_CACHE_LOCK_CONNECTION=mysql
SESSION_CONNECTION=mysql

MEMCACHED_HOST=127.0.0.1

REDIS_CLIENT=phpredis
REDIS_HOST=127.0.0.1
REDIS_PASSWORD=null
REDIS_PORT=6379

MAIL_MAILER=log
MAIL_SCHEME=null
MAIL_HOST=127.0.0.1
MAIL_PORT=2525
MAIL_USERNAME=null
MAIL_PASSWORD=null
MAIL_FROM_ADDRESS="hello@example.com"
MAIL_FROM_NAME="${APP_NAME}"

# Platform onboarding
PLATFORM_ADMIN_EMAIL=custom@termresult.com

# Tenant portal links in approval email
# Local dev example:
# TENANT_PORTAL_URL_TEMPLATE=http://{subdomain}.localhost:5173
# Production example:
# TENANT_PORTAL_URL_TEMPLATE=https://{subdomain}.termresult.com
TENANT_PORTAL_URL_TEMPLATE=https://{subdomain}.termresult.com

# CORS (comma-separated).
# Production example:
# CORS_ALLOWED_ORIGINS=https://termresult.com,https://*.termresult.com,https://api.termresult.com
CORS_ALLOWED_ORIGINS=*

# Paystack
PAYSTACK_SECRET_KEY=

AWS_ACCESS_KEY_ID=
AWS_SECRET_ACCESS_KEY=
AWS_DEFAULT_REGION=us-east-1
AWS_BUCKET=
AWS_USE_PATH_STYLE_ENDPOINT=false

VITE_APP_NAME="${APP_NAME}"
